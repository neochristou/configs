	using namespace std;
	
	
	int main() {
	    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
	    int n{};
	    int q{};
	    std::cin >> n;
	    std::cin >> q;
	    std::vector<int> a[n];
	    int in{};
	    int v_size{};
	    for (int i = 0; i < n; i++){
	        std::cin >> v_size;
	        for (int j = 0; j < v_size; j++){
	            std::cin >> in;
	            a[i].push_back(in);
	        }
