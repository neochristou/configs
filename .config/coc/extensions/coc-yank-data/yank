	u = var('u')
	e = 65537
	found = False
	for k in range(1, 1000):
	    sol = solve([a * (k*a+b) == n1 - n2 - a * u - (k*a+b)*n2/u],u)
	    for i in range(2):
	        try:
	            q2 = Integer(sol[i].right())
	            q1 = q2 + (k*a+b)
	            assert is_prime(q1)
	            assert is_prime(q2)
	            p1 = n1/q1
	            phi = (p1-1) * (q1-1)
	            d = inverse_mod(e, phi)
	            pt = pow(ct, d, n1)
	            print('K:', k)
	            print('Phi: ', phi)
	            print('D: ', d)
	            print('Plaintext:', pt)
	            found = True
	            break
