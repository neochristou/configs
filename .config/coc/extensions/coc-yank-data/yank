c5848bcd1d4cea9553ebadc6ea99b70d|file:///home/neo/cryptohack/misc/prngs/sol_sleeve.py|21|1|V|python
	class CustomRNG:
	    def __init__(self, first_state, P, Q):
	        self.seed = seed
	        self.P = P
	        self.Q = Q
	        self.first_state = first_state
	
	    def first_output(self):
	        s = self.first_state
	        self.seed = s
	        r = (s * self.Q).x
	        return r & (2**(8 * 30) - 1)
	
	    def next(self):
	        t = self.seed
	        s = (t * self.P).x
	        self.seed = s
	        r = (s * self.Q).x
	        return r & (2**(8 * 30) - 1)
