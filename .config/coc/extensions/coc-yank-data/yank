	           ret.a.push_back(v);
de026b94f31848335dd1460fa34d7072|file:///home/neo/cplusplus/hr.cpp|8|1|V|cpp
	class Matrix{
	   public:
	       vector<vector<int>> a;
	       Matrix operator+(const Matrix &m){
	           Matrix ret;
	           for (int i = 0; i < a.size(); i++){
	               vector<int> v{};
	               for (int j = 0; j < a[0].size(); j++)
	                   v.push_back(a[i][j] + m.a[i][j]);
	               ret.a.push_back(v);
	           }
	           return ret;
	       }
	};
33ba4b2c1ead26a969adfb42b2774ed3|file:///home/neo/cplusplus/hr.cpp|1|1|V|cpp
	#include <cmath>
	#include <cstdio>
	#include <vector>
	#include <iostream>
	#include <algorithm>
	using namespace std;
	
	class Matrix{
	   public:
	       vector<vector<int>> a;
	       Matrix &operator+(const Matrix &m){
	           Matrix ret;
	           for (int i = 0; i < a.size(); i++){
	               vector<int> v{};
	               for (int j = 0; j < a[0].size(); j++)
	                   v.push_back(a[i][j] + m.a[i][j]);
	               ret.a.push_back(v);
	           }
	           return ret;
	       }
	};
	
	
	int main () {
	   int cases,k;
	   cin >> cases;
	   for(k=0;k<cases;k++) {
	      Matrix x;
	      Matrix y;
	      Matrix result;
