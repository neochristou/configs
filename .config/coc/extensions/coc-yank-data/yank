	def hashThenXor(xorkey, ctstr)
	    ct = binascii.unhexlify(ctstr)
	    b = bytes(ct)
	    md5sum = hashlib.md5(b)
	    hashmd5 = md5sum.hexdigest()
	    print(hashmd5)
	    plain = hex(int(xorkey, 16) ^ int(hashmd5, 16)).replace('0x','')
	    print(plain)
