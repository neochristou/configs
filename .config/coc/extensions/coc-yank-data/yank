	        keys = self.key_expansion(key)
	        for i in range(65536):
	            pt = os.urandom(8)
	            ct = pt
	            for i in range(ROUNDS):
	                ct = self.apply_sbox(ct)
	                ct = self.apply_perm(ct)
	                ct = self.apply_key(ct, keys[i])
