	def rc4_dec(text, key):
	    S = [i for i in range(256)]
	    j = 0
	    out = bytearray()
	
	    #KSA Phase
	    for i in range(256):
	        j = (j + S[i] + key[i % len(key)] ) % 256
	        S[i] , S[j] = S[j] , S[i]
	
	    #PRGA Phase
	    i = j = 0
	    for char in text:
	        i = ( i + 1 ) % 256
	        j = ( j + S[i] ) % 256
	        S[i] , S[j] = S[j] , S[i]
	        out.append(ord(char) ^ S[(S[i] + S[j]) % 256])
	
	    return(out)
