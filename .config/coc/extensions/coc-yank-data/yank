	            cp = capacity;
	            tail = NULL;
	            head = NULL;
	        }
	
	        virtual void set(int key, int value){
	            if (mp.empty()){
	                Node *node = new Node(key, value);
	                head = node;
	                tail = node;
	                mp[key] = node;
	                return;
	            }
	            if ( auto it = mp.find(key);  it != mp.end()){
	                Node *cur = it->second;
	                cur->value = value;
	                if (cur == head)
	                    return;
	                cur->prev->next = cur->next;
	                if (cur == tail)
	                    tail = tail->prev;
	                else
	                    cur->next->prev = cur->prev;
	                cur->next = head;
	                head = cur;
	                cur->prev = NULL;
	            
	            } else{
	                Node *node = new Node(head->prev, head, key, value);
	                head->prev = node;
	                head = node;
	                mp[key] = node;
	                if (mp.size() > cp){
