	class RNG:
	    def __init__(self, seed, P, Q):
	        self.seed = seed
	        self.P = P
	        self.Q = Q
	
	    def next(self):
	        t = self.seed
	        s = (t * self.P).x
	        self.seed = s
	        r = (s * self.Q).x
	        return r & (2**(8 * 30) - 1)
