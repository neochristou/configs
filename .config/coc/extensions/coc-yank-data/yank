	n = 
	n_bin = bin(n)[2:]
	try = n_bin[:128]
	to_try = []
	while try.nbits() > 62:
	    to_try.append(try)
	    if try & 1:
	        to_try.append(try-1)
	    else:
	        to_try.append(try+1)
	    try /= 2
	
	test_factors = []
	for test in to_try:
	    factors = ecm.factor(test)
	    if len(factors) == 2:
	        test_factors.append(factors)
	
