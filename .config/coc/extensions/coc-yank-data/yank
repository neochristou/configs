	def send_payload(payload, interactive=False):
	    # if b'\n' in payload:
	    #     return None
	    r.recvuntil('?\n')
	    r.sendline(payload)
	    r.recvuntil('back ')
	    if (interactive):
	        r.interactive()
	        return
	    data = r.recvuntil(b'!!!', drop=True)
	    return data
