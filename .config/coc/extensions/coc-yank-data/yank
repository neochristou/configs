	possible = string.ascii_letters+string.digits
	possible = list(map(''.join, it.product(possible, repeat=4)))
	pool = mp.Pool(32)
	for c in pool.imap_unordered(check, possible, chunksize=100000):
	    if c:
	        log.info('Solved - %s' % (c))
	        r.sendline(c)
	        break
	pool.close()
