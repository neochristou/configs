	    if not isinstance(bytes1, bytes) or not isinstance(bytes2, bytes):
	        raise TypeError
	    if len(bytes1) != len(bytes2):
	        raise Exception('Chunks must have the same length')
	    xored = bytearray(bytes1)
	    for i, b in enumerate(bytes2):
	        xored[i] ^= b
	    return bytes(xored)
