	def encrypt(ptxt, key):
	    ptxt = ptxt.lower()
	    key = ''.join(key[i % len(key)] for i in range(len(ptxt))).lower()
	    ctxt = ''
	    for i in range(len(ptxt)):
	        if ptxt[i] == '_':
	            ctxt += '_'
	            continue
	        x = chr_to_num[ptxt[i]]
	        y = chr_to_num[key[i]]
	        ctxt += num_to_chr[(x + y) % 26]
	    return ctxt
