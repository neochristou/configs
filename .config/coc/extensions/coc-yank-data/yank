	class RNG:
	    mul = random.getrandbits(60)
	    inc = random.getrandbits(60)
	    mod = 2**61 - 1  # 9th mersenne prime
	
	    def __init__(self, seed):
	        self.state = seed
	
	    def next(self):
	        self.state = (self.state * self.mul + self.inc) % self.mod
	        return self.state
	
