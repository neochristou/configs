	async function timePost(username, password) {
	  let totalTime = 0;
	  let t0, t1, elapsed;
	  let repeats = 1000;
	  for (let i = 0; i < repeats; i++) {
	    t0 = performance.now();
	    await submitPassword(username, password);
	    t1 = performance.now();
	    elapsed = t1 - t0;
	    totalTime += elapsed;
	  }
	  return Promise.resolve(totalTime/repeats);
	}
