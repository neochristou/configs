	vector<int> parseInts(string str) {
	    stringstream s(str);
	    vector<int> v;
	    int n{};
	    char com {','};
	    while (com == ','){
	        s >> n;
	        v.push_back(n);
	        s >> com;
	    }
	    return v;
		// Complete this function
