	def gen():
	    from Crypto.Util.number import getStrongPrime
	    e = 65537
	    Ns = []
	    for i in range(K):
	        p = getStrongPrime(2048)
	        q = getStrongPrime(2048)
	        Ns.append(p*q)
	    return e,Ns
	
