	app = [''.join(i) for i in itertools.product(alphabets, repeat = 3)]
	sec = 'ractf{'
	while True:
	    enc_len = (len(sec) + 1) * 3 - 1
	    found = False
	    print(ct[:enc_len])
	    for c in string.printable:
	        r.recvuntil('with: ')
	        r.sendline(sec + c)
	        r.recvuntil('encrypt: ')
	        r.sendline(pt)
	        r.recvuntil('is: ')
	        enc = r.recvline().decode('ascii')
	        # print(enc[:enc_len])
	        # print(c)
	        if enc[:enc_len] == ct[:enc_len]:
	            found = True
	            sec += c
	            print(sec)
	            break
	    if not found:
	        break
