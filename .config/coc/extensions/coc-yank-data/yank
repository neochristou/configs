	# r = remote('socket.cryptohack.org', 13370, level = 'debug')
	r = remote('socket.cryptohack.org', 13370)
	
	def json_recv():
	    line = r.recvline()
	    return json.loads(line.decode())
	
	def json_send(hsh):
	    request = json.dumps(hsh).encode()
	    r.sendline(request)
	
