	K = 64
	
	msg = b'hello'
	keystream = []
	key = sha256(msg).digest()[:16]
	E = ARC4.new(key)
	
	for i in range(K):
	    keystream.append(unpack('Q', E.encrypt(pack('Q',0)))[0])
	
	ys = []
	for i in range(K-1):
	    ys.append(0)
	
	E = ARC4.new(key)
	v = 0
	cur = v
	curs =[]
	curs.append(v)
	for i in range(K-1):
	    pt = (ys[i]^cur)%(1<<64)
	    ct = unpack('Q', E.encrypt(pack('Q',pt)))[0]
	    cur = ct
	    curs.append(cur)
	
