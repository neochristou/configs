	def send_read():
	    filename = b'/session/123'
	    read_packet = (
	        p32(0) + #offset
	        p32(23) + #fd
	        p32(100) + #fd defined length
	        p32(len(filename)) + #length
	        filename + #path
	        b''
	    )
	
	    payload = (
	        p32(0x50494843) +
	        p32(types['READ']) +
	        p32(123) + #session
	        p64(1) + #callback
	        read_packet.ljust(DATA_MAX_SIZE, b'\x00')+
	        b''
	    )
	
	    print('Payload:', payload)
	
	    return payload
