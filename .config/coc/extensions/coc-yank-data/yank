	r.recvuntil('choice: ')
	r.sendline('2')
	r.recvuntil('seating: ')
	ct = str(r.recvline())[2:-3]
	
	flag = ''
	pt = ["A"] * 46
	for i in range(len(ct)):
	    r.sendline('1')
	    r.recvuntil('bus?: ')
	    pt[i] = "B"
	    send = ''.join(pt)
	    r.sendline(send)
	    r.recvuntil('seating: ')
	    seating = str(r.recvline())[2:-3]
	    seat = seating.index("B")
	    flag += ct[seat]
	    print(flag)
	    pt[i] = "A"
	
