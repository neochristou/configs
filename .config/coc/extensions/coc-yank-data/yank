	eliminated = {}
	for i in range(20):
	    eliminated[i] = set()
	
	r.recvuntil('No leaks\n')
	for i in range(2000):
	    json_send({'msg': 'request'})
	    response = json_recv()
	    if 'ciphertext' in response:
	        ciphertext = base64.b64decode(response['ciphertext'])
	        for i, c in enumerate(ciphertext):
	            eliminated[i].add(c)
	
	# print(eliminated[0])
	flag = ''
	for i in range(20):
	    elim = eliminated[i]
	    for c in string.printable:
	        if ord(c) not in elim:
	            flag += c
	            break
	
	print(flag)
