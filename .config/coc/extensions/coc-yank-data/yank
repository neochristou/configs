	import random
	from Crypto.Util.number import *
	import gmpy2
	
	a = 0xe64a5f84e2762be5
	chunk_size = 64
	
	def gen_prime(bits):
	  s = random.getrandbits(chunk_size)
	
	  while True:
	    s |= 0xc000000000000001
	    print(hex(s))
	    p = 0
	    for _ in range(bits // chunk_size):
	      p = (p << chunk_size) + s
	      s = a * s % 2**chunk_size
	    print(bin(p)[2:])
	    if gmpy2.is_prime(p):
	      return p
